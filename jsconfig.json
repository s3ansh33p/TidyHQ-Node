{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "maxNodeModuleJsDepth": 1,
        "moduleResolution": "node",
        "checkJs": true,
        "baseUrl": ".",
        "paths": {
            "*": ["node_modules/*"]
        }
    },
    "exclude": ["docs", "node_modules"],
    "include": ["**/*.js"]
}